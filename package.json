{
  "name": "printpress",
  "version": "1.0.0",
  "description": "Pinterest Image Gallery - MERN Stack Application",
  "scripts": {
    "install-all": "cd backend && npm install && cd ../frontend && npm install",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm run dev",
    "client": "cd frontend && npm start",
    "start": "cd backend && npm start"
  },
  "keywords": ["pinterest", "mern", "react", "express", "mongodb"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.1"
  }
}
